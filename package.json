{
  "engines": {
    "composer": "^0.20.8"
  },
  "name": "pension",
  "version": "0.0.1",
  "description": "A Hyperledger Composer network for distributing pension benefits",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/pension.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "Thomas Billiet",
  "email": "hello@thomasbilliet.com",
  "license": "MIT",
  "devDependencies": {
    "composer-admin": "^0.20.8",
    "composer-cli": "^0.20.8",
    "composer-client": "^0.20.8",
    "composer-common": "^0.20.8",
    "composer-connector-embedded": "^0.20.8",
    "composer-cucumber-steps": "^0.20.8",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
}
